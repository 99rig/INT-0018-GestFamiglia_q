{
  "appId": "com.mumble.crazyfamily",
  "appName": "MyCrisisFamily",
  "webDir": "www",
  "bundledWebRuntime": false,
  "backgroundColor": "#239db0",
  "server": {
    "iosScheme": "capacitor",
    "androidScheme": "https"
  },
  "ios": {
    "backgroundColor": "#239db0",
    "contentInset": "automatic",
    "preferredContentMode": "mobile",
    "allowsLinkPreview": true,
    "scheme": "App",
    "overrideUserAgent": "MyCrisisFamily/1.0"
  },
  "android": {
    "backgroundColor": "#239db0",
    "allowMixedContent": false,
    "webContentsDebuggingEnabled": false,
    "minWebViewVersion": 60,
    "flavor": "main"
  },
  "plugins": {
    "Camera": {
      "presentationStyle": "fullscreen",
      "allowEdit": true,
      "resultType": "uri"
    },
    "PushNotifications": {
      "presentationStyle": "badge|sound|alert"
    },
    "SplashScreen": {
      "launchAutoHide": true,
      "backgroundColor": "#239db0",
      "showSpinner": false,
      "androidSpinnerStyle": "large",
      "iosSpinnerStyle": "small",
      "spinnerColor": "#ffffff",
      "splashFullScreen": true,
      "splashImmersive": true
    },
    "StatusBar": {
      "style": "default",
      "backgroundColor": "#239db0"
    }
  }
}
